        },
        "text_8htgPp": {
          "type": "text",
          "settings": {
            "text": "<p>We do.<\/p><p>We screwed up by trying to follow the industry.  We were trying to stay price competitive when we didn't have to.<\/p>"
          }
        }
      },
      "block_order": [
